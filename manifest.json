{
    "manifest_version": 2,
    "name": "Social Media Manager",
    "description": "Youre favorite Social Media apps in one place!",
    "version": "3.1.0",
    "icons": {
        "128": "blueFavicons/blue128.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "blueFavicons/favicon-32x32.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}